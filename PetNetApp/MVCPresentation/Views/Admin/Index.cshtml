@model IEnumerable<MVCPresentation.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Register New Employee User", "RegisterEmployeeUser", "Account", routeValues: null, htmlAttributes: new { id = "registerLink"})
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UsersId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShelterId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GivenName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FamilyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PronounId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GenderId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Zipcode)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.UsersId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ShelterId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.GivenName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.FamilyName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PronounId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.GenderId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Zipcode)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
